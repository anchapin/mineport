{
  "name": "minecraft-mod-converter",
  "version": "0.1.0",
  "description": "A hybrid translation pipeline to convert Minecraft Java Edition mods to Bedrock Edition addons",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "vitest run --coverage && node scripts/coverage-report.js",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:security": "vitest run tests/security",
    "security:audit": "npm audit --audit-level=high",
    "security:scan": "npm run security:audit && npm run test:security",
    "benchmark": "ts-node tests/benchmark/conversion-benchmark.ts",
    "benchmark:report": "ts-node scripts/generate-benchmark-report.js",
    "deps:analyze": "node scripts/analyze-dependencies.js",
    "deps:validate": "node scripts/validate-dependencies.js",
    "docs:validate": "node scripts/validate-jsdoc.js",
    "docs:add": "node scripts/add-jsdoc.js",
    "docs:add:dry-run": "node scripts/add-jsdoc.js --dry-run",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "minecraft",
    "mod",
    "converter",
    "java",
    "bedrock",
    "addon"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "java-parser": "^2.0.4",
    "mongodb": "^6.1.0",
    "octokit": "^5.0.3",
    "redis": "^4.6.10",
    "unzipper": "^0.10.14",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "c8": "^8.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
