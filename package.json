{
  "name": "minecraft-mod-converter",
  "version": "0.1.0",
  "description": "A hybrid translation pipeline to convert Minecraft Java Edition mods to Bedrock Edition addons",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "vitest run --coverage && node scripts/coverage-report.cjs",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:integration:watch": "vitest --config vitest.integration.config.ts",
    "test:security": "vitest run tests/security",
    "test:performance": "vitest run tests/benchmark",
    "test:e2e": "vitest run tests/integration/end-to-end-conversion.test.ts",
    "test:modporter-ai": "vitest run tests/security/modporter-ai-security.test.ts tests/unit/modules/ingestion/FileProcessor.enhanced.test.ts tests/unit/modules/ingestion/JavaAnalyzer.enhanced.test.ts tests/benchmark/modporter-ai-performance.test.ts",
    "test:comprehensive": "node scripts/run-comprehensive-tests.js",
    "security:audit": "npm audit --audit-level=high",
    "security:scan": "node scripts/artifact-security.js scan",
    "security:patch": "node scripts/security-patch-automation.js",
    "security:patch:dry-run": "node scripts/security-patch-automation.js --dry-run",
    "benchmark": "ts-node tests/benchmark/conversion-benchmark.ts",
    "benchmark:report": "ts-node scripts/generate-benchmark-report.js",
    "deps:analyze": "node scripts/analyze-dependencies.js",
    "deps:validate": "node scripts/validate-dependencies.js",
    "deps:conflicts": "node scripts/dependency-conflict-resolver.js",
    "deps:conflicts:dry-run": "node scripts/dependency-conflict-resolver.js --dry-run",
    "deps:conflicts:fix": "node scripts/dependency-conflict-resolver.js --auto-fix",
    "docs:validate": "node scripts/validate-jsdoc.js",
    "docs:validate:links": "node scripts/validate-docs-links.js",
    "docs:validate:versions": "node scripts/validate-version-consistency.js",
    "docs:validate:markdown": "node scripts/validate-markdown-lint.js",
    "docs:validate:sections": "node scripts/validate-required-sections.js",
    "docs:validate:all": "npm run docs:validate:links && npm run docs:validate:versions && npm run docs:validate:markdown && npm run docs:validate:sections",
    "docs:add": "node scripts/add-jsdoc.js",
    "docs:add:dry-run": "node scripts/add-jsdoc.js --dry-run",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "deploy": "./scripts/deploy-modporter-ai.sh",
    "deploy:canary": "./scripts/canary-deployment.sh",
    "deploy:staging": "./scripts/deploy-modporter-ai.sh --env staging",
    "deploy:production": "./scripts/deploy-modporter-ai.sh --env production",
    "rollback": "./scripts/rollback-deployment.sh",
    "rollback:emergency": "./scripts/rollback-deployment.sh --type emergency",
    "rollback:gradual": "./scripts/rollback-deployment.sh --type gradual",
    "db:migrate": "node scripts/run-migrations.js up",
    "db:rollback": "node scripts/run-migrations.js down",
    "db:status": "node scripts/run-migrations.js status",
    "db:validate": "node scripts/run-migrations.js validate",
    "db:check": "pg_isready -h ${DB_HOST:-localhost} -p ${DB_PORT:-5432}",
    "test:smoke": "vitest run tests/deployment/smoke-tests.test.ts",
    "test:pre-deploy": "npm run test:unit && npm run test:security && npm run db:validate",
    "test:config": "vitest run tests/unit/services/ConfigurationService.test.ts",
    "test:integration:staging": "TEST_BASE_URL=${TEST_BASE_URL:-https://staging.mineport.com} vitest run tests/integration",
    "test:performance:staging": "TEST_BASE_URL=${TEST_BASE_URL:-https://staging.mineport.com} vitest run tests/benchmark/performance-optimization.test.ts",
    "config:validate": "curl -f http://localhost:3000/config/validate || echo 'Service not running'",
    "health:check": "curl -f http://localhost:3000/health",
    "health:ready": "curl -f http://localhost:3000/ready",
    "health:live": "curl -f http://localhost:3000/live",
    "validate:deployment": "node scripts/validate-deployment.js",
    "db:migrate:dry-run": "node scripts/run-migrations.js dry-run",
    "artifacts:create": "node scripts/artifact-manager.js create",
    "artifacts:validate": "node scripts/artifact-manager.js validate",
    "artifacts:upload": "node scripts/artifact-manager.js upload",
    "artifacts:metadata": "node scripts/artifact-manager.js metadata",
    "artifacts:all": "node scripts/artifact-manager.js all",
    "release:notes": "node scripts/generate-release-notes.js generate",
    "release:changelog": "node scripts/generate-release-notes.js changelog",
    "release:prepare": "npm run test:comprehensive && npm run artifacts:create && npm run release:notes",
    "security:artifacts": "node scripts/artifact-security.js secure",
    "security:sign": "node scripts/artifact-security.js sign",
    "security:validate": "node scripts/artifact-security.js validate",
    "hooks:install": "pre-commit install",
    "hooks:run": "pre-commit run --all-files",
    "hooks:update": "pre-commit autoupdate",
    "hooks:gitleaks": "pre-commit run gitleaks --all-files"
  },
  "keywords": [
    "minecraft",
    "mod",
    "converter",
    "java",
    "bedrock",
    "addon"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "java-parser": "^2.0.4",
    "jszip": "^3.10.1",
    "lru-cache": "^10.4.3",
    "mongodb": "^6.1.0",
    "octokit": "^5.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "redis": "^4.6.10",
    "unzipper": "^0.10.14",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/adm-zip": "^0.5.7",
    "@types/express": "^4.17.20",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.8.7",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^8.0.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "sharp": "^0.34.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}
